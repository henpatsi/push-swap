NAME = push_swap

LIBFT = ./libft/libft.a

SOURCES = push_swap.c handle_input.c \
		  ./stack/stack_utils.c ./stack/check_state.c \
		  ./sorting/exec_command.c ./sorting/p_s_commands.c ./sorting/r_commands.c \
		  ./sorting/tiny_sort.c ./sorting/push_smallest_sort.c ./sorting/radix_sort.c \
		  ./stack/test_stack_print.c

BONUS_SOURCE = checker_bonus.c

OBJECTS = $(SOURCES:.c=.o)

BONUS_OBJECTS = $(BONUS_SOURCE:.c=.o)

CFLAGS += -Wall -Wextra -Werror -I ./includes/

all: $(NAME)

$(NAME): $(OBJECTS) $(LIBFT)
	cc $(OBJECTS) $(LIBFT) -o $(NAME)

$(OBJECTS): $(SOURCES)

$(LIBFT):
	make -C ./libft

bonus:


clean:
	make clean -C ./libft
	rm -f $(OBJECTS)

fclean: clean
	rm -f $(LIBFT)
	rm -f $(NAME)

re: fclean all

debug:
	cc $(OBJECTS) $(LIBFT) -g -o $(NAME)

test_inputs: $(NAME)
	./$(NAME) 1 2 3
	./$(NAME) 0 1 2
	./$(NAME) 1 2 1
	./$(NAME) 1 2 2147483647
	./$(NAME) -2147483648 1 2
	./$(NAME) 1 2 2147483648
	./$(NAME) -2147483649 1 2
	./$(NAME) 1 2 a
	./$(NAME) 1 2 +3
	./$(NAME) 1 2 3a

test: $(NAME)
